{
  "name": "soothsaying",
  "version": "0.1.2",
  "description": "This API is used to create deck of Tarot cards, shuffle, and deal 3 cards for a Tarot reading.",
  "private": false,
  "engines": {
    "node": "16.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/free-ignorance/soothsaying.git"
  },
  "bugs": {
    "url": "https://github.com/free-ignorance/soothsaying/issues"
  },
  "contributors": [
    "Beau Bouchard <https://github.com/BeauBouchard>"
  ],
  "scripts": {
    "start": "ts-node src/app.ts",
    "build": "tsc",
    "serve": "node dist/app.js",
    "test": "jest",
    "test:coverage": "jest --coverage | ./node_modules/.bin/codecov",
    "test:lint": "eslint . 'src/**/*.ts'",
    "test:lint:fix": "eslint . --fix 'src/**/*.ts'"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "helmet": "^7.1.0",
    "winston": "^3.12.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.28",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^6.13.1",
    "codecov": "^3.8.3",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tslint": "^6.1.3",
    "typescript": "^5.4.2"
  }
}
